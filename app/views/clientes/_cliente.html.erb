<div id="<%= dom_id cliente %>" class="container mt-4">
  <div class="table-responsive">
    <table class="table table-bordered table-striped mx-auto col-6">
      <thead class="thead-dark">
        <tr>
          <th colspan="2" class="text-center">Informações do Cliente</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td class="font-weight-bold">Nome:</td>
          <td><%= cliente.nome %></td>
        </tr>

        <tr>
          <td class="font-weight-bold">Telefone:</td>
          <td><%= cliente.telefone %></td>
        </tr>

        <tr>
          <td class="font-weight-bold">Cpf:</td>
          <td><%= cliente.cpf %></td>
        </tr>

        <tr>
          <td class="font-weight-bold">Rg:</td>
          <td><%= cliente.rg %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
